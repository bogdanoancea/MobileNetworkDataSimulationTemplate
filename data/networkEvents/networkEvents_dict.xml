<?xml version="1.0" encoding="UTF-8"?>
<networkEvents>
	<specs_events>
		<specs_file>
			<fileName>networkEvents.csv</fileName>
		</specs_file>
		<specs_mno>
			<name> MNO1</name>
		</specs_mno>
		
		<specs_time type="Discrete">
			<timeColName>time</timeColName>
			<time_start>0</time_start>
			<time_end>890</time_end>
			<time_increment>10</time_increment>
			<time_unit>s</time_unit>
		</specs_time>

		<specs_event eventType = "CellID">
			<cellIDColName>CellID</cellIDColName>
			<cellID_value_type>string</cellID_value_type>
			<eventCodeColName>eventCode</eventCodeColName>
			<eventCode_value_type>integer</eventCode_value_type>
		</specs_event>
		
		<specs_device>
			<devColName>device</devColName>
			<devID_value_type>string</devID_value_type>
			<description> These are the ID of the devices</description>
		</specs_device>

		<specs_coords CRSType = "None">
			<xColName>x</xColName>
			<x_value_type>decimal</x_value_type>
			<yColName>y</yColName>
			<y_value_type>decimal</y_value_type>
		</specs_coords>
		
		<specs_tile>
			<tileColName>tile</tileColName>
			<tileID_value_type>unsignedInt</tileID_value_type>
		</specs_tile>
	</specs_events>	


</networkEvents>