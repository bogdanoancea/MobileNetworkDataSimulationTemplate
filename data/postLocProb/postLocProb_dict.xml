<?xml version="1.0" encoding="UTF-8"?>
<posteriorLocationProbabilities>
	<specs_file>
		<fileName>postLocProb_HMM_RSS-network.csv</fileName>
	</specs_file>
	
	<specs_device>
		<devColName>device</devColName>
		<devID_value_type>string</devID_value_type>
		<description> These are the ID of the devices</description>
	</specs_device>


	<specs_time type="Discrete">
		<timeColName>time</timeColName>
		<time_start>0</time_start>
		<time_end>890</time_end>
		<time_increment>10</time_increment>
		<time_unit>s</time_unit>
	</specs_time>

	<specs_tile>
		<tileColName>tile</tileColName>
		<tileID_value_type>unsignedInt</tileID_value_type>
	</specs_tile>

	<specs_cells>
		<cellIDColName>CellID</cellIDColName>
		<cellID_value_type>string</cellID_value_type>
	</specs_cells>
	
	<specs_prob>
        	<probColName>postLocProb</probColName>
	        <method name = "HMM">
					<prior>network</prior>
					<transition>rectangle</transition>
					<emission>RSS</emission>
	        </method>
	</specs_prob>
</posteriorLocationProbabilities>
